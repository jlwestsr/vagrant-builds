    // DOM
    $(document).ready(function() {    
        // hide the weirdness
        $('#ui-datepicker-div').hide();

        //required fields
        $('#submitButton').click( function(){
            var message = [];

            // first name, required
            if(
                $('#vFirstName').length &&
                $('#vFirstName').val() == '*First Name'
            ){
                // message
                message.push('First Name');
            }

            // last name, required
            if(
                $('#vLastName').length &&
                $('#vLastName').val() == '*Last Name'
            ){
                // message
                message.push('Last Name');
            }

            if(
                (
                    $("input:radio[name=ContactBy]:checked").length &&
                    $("input:radio[name=ContactBy]:checked").val() == 'email' &&
                    $('#vEmail').val() == 'Email Address'
                ) ||
                (
